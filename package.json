{
  "name": "journal",
  "version": "1.0.0",
  "description": "Personal Project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jad-rodriguez/Journal.git",
    "homepage": "https://github.com/jad-rodriguez/Journal#readme"
  },
  "scripts": {
    "build": "webpack --config ./client/webpack.config.js",
    "dev:client": "npm run build -- --watch",
    "dev:server": "nodemon server",
    "dev": "run-p dev:client dev:server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint . --fix",
    "start": "node server",
    "test": "jest --watchAll --verbose"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "keywords": [],
  "author": "Jad Rodriguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jad-rodriguez/Journal/issues"
  },
  "dependencies": {
    "dotenv": "^15.0.0",
    "express": "^4.17.1",
    "knex": "^1.0.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "babel-loader": "^8.2.2",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "superagent": "^5.3.1",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
  }
}
